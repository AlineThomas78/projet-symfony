<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}Beauty massage {% endblock %}</title>

        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        <header>
            {% include 'base/nav.html.twig'%}
        </header>
        {% for status, messages in app.flashes %}
            {% for message in messages %}
                <div class="alert alert-{{status}}" style="z-index: 999;"> {{message}}</div>
            {% endfor %}
        {% endfor %}
        {% block body %} {% endblock %}

        

        {% include 'base/footer.html.twig'%}
    </body>
</html>
